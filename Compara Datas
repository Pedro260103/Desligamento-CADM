CompareDates(dataP as String, dataT as String) function as Boolean
 ' Converts string dates to date objects
    Dim dataP_Obj As Date
    Dim dataT_Obj As Date
    
 If IsDate(dataP) and IsDate(dataT), then
        dataP_Obj = CDate(dataP)
        dataT_Obj = CDate(dataT)
        
 ' Compares dates and returns True if dataP is greater than dataT
        CompararDatas = (dataP_Obj > dataT_Obj)
 But
 ' If one of the dates is not valid, returns False
 CompararDatas = False
 End If
End function
